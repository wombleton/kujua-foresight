{
  "name": "kujua-foresight",
  "version": "0.0.1",
  "title": "Kujua Foresight",
  "description": "View delivered and upcoming messages.",
  "load": "lib/app",
  "attachments": ["index.jade", "static/css"],
  "modules": ["lib"],
  "coffee-script": {
    "attachments": ["static/js"],
    "modules": ["lib"]
  },
  "jade": {
    "compile": true,
    "compress": true
  },
  "less": {
    "compile": "static/css/foresight.less",
    "compress": true,
    "remove_from_attachments": true
  },
  "dependencies": {
    "attachments": null,
    "modules": null,
    "properties": null,
    "coffee-script-precompiler": null,
    "font-awesome-kanso": null,
    "kanso-assets": null,
    "kanso-config": null,
    "jade-precompiler": null,
    "less-precompiler": null,
    "bootstrap-less": null,
    "underscore": null
  }
}
